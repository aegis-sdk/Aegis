{
  "name": "@aegis-sdk/core",
  "version": "0.0.0",
  "description": "Streaming-first prompt injection defense for AI applications",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "patterns"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "llm",
    "prompt-injection",
    "security",
    "streaming",
    "defense",
    "guard",
    "openai",
    "anthropic",
    "vercel-ai"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aegis-sdk/aegis",
    "directory": "packages/core"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}
